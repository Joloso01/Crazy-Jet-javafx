<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainWindow">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <AnchorPane prefHeight="358.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
               <image>
                  <Image url="@images/fondo_Intro.jpg" />
               </image>
            </ImageView>
            <Label layoutX="247.0" layoutY="40.0" text="Crazy Jet" textAlignment="CENTER" textOverrun="LEADING_ELLIPSIS">
               <font>
                  <Font name="Bitstream Vera Serif Bold" size="21.0" />
               </font>
            </Label>
            <Button fx:id="btnjugar" layoutX="267.0" layoutY="232.0" mnemonicParsing="false" onAction="#jugar" text="Jugar" />
            <Button fx:id="btnScore" layoutX="240.0" layoutY="271.0" mnemonicParsing="false" onAction="#showScore" text="Puntuaciones" />
            <Button fx:id="exit" layoutX="270.0" layoutY="309.0" mnemonicParsing="false" onAction="#exitGame" text="Salir" />
         </children>
      </AnchorPane>
   </children>
</VBox>
